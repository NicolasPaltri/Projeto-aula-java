
public class AppString {

	public static void main(String[] args) {
		Pelicano[] filhotes= new Pelicano[3];
		
		Pelicano p1 = new Pelicano();
		Pelicano p2 = new Pelicano();
		
		filhotes[0] = p1;
		filhotes[1] = p2;
		filhotes[2] = new Pelicano();
		
		System.out.println(filhotes [0].getCor());
	}

}