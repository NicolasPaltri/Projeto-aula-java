package zoologico;

public class Zoologico {

	public static void main(String[] args) {
		Pelicano p1, p2;
		
		p1 = new Pelicano();
		p2 = new Pelicano();
		
		p1.cor ="Branco";
		p1.peso = 40;
		p2.cor ="Preto";
		p2.peso = 30;
		
		p1.voar ();
		p2.comer();
		p1.definirNome(" Kleid");
		
		System.out.println("Os pelicanos são " +  p1.cor + " e " + p2.cor );
		System.out.println("Outra forma de retorna a cor" + p2.retornaCor()+(""));

	}
	public void comer() {
		System.out.println("O pelicano está comendo");
		
	}
	

}
