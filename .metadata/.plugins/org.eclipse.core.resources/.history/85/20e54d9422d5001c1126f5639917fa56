package zoologico;

public class Zoologico {

	public static void main(String[] args) {
		Pelicano p1, p2;
		
		p1 = new Pelicano();
		p2 = new Pelicano();
		
		p1.cor ="Branco";
		p2.cor ="preto";
		
		p1.voar ();
		p2.comer();
		
		System.out.println("Os pelicanos são " +  p1.cor + " e " + p2.cor );
		System.out.println("Os pelicanos estão " +  p1.comer() + " e " + p2.comer() );
	}

}
